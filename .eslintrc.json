{
    "extends": [
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/babel",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard",
      "prettier/unicorn",
      "prettier/vue"
    ],
    "rules": {
      "eqeqeq": "off",
      "class-methods-use-this": "off"
    },
      "parserOptions": {
          "ecmaVersion": 2018
      },
  
      "env": {
          "es6": true
      },
      "parser": "babel-eslint"
  
}